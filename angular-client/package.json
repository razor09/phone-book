{
  "name": "angular-client",
  "scripts": {
    "start": "../express-api/node_modules/.bin/pm2 start server.js --name angular-client",
    "stop": "../express-api/node_modules/.bin/pm2 delete angular-client",
    "launch": "node server.js",
    "dev": "ng serve --port=4000",
    "prod": "ng build --prod",
    "update": "rm -rf node_modules && npm ci"
  },
  "dependencies": {
    "@angular/animations": "10.1.6",
    "@angular/common": "10.1.6",
    "@angular/compiler": "10.1.6",
    "@angular/core": "10.1.6",
    "@angular/forms": "10.1.6",
    "@angular/platform-browser": "10.1.6",
    "@angular/platform-browser-dynamic": "10.1.6",
    "@angular/router": "10.1.6",
    "core-js": "3.6.5",
    "font-awesome": "4.7.0",
    "node-static": "0.7.11",
    "rxjs": "6.6.3",
    "socket.io-client": "2.3.1",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.7",
    "@angular/cli": "10.1.7",
    "@angular/compiler-cli": "10.1.6",
    "@angular/language-service": "10.1.6",
    "@types/node": "14.11.10",
    "@types/socket.io-client": "1.4.34",
    "ts-node": "9.0.0",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  }
}
